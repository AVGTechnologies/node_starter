    rabbit_logs_port:             5672
    rabbit_logs_virtual_host:     host
    rabbit_finalci_username:      username
    rabbit_finalci_password:      password
    rabbit_finalci_address:       localhost
    rabbit_finalci_port:          5672
    rabbit_finalci_virtual_host:  host
    rabbit_finalci_exchange:      reporting
    rabbit_finalci_routing_key:   reporting.builds.builds
